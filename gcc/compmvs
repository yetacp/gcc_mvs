do

echo "Cross-Compiling - $file"
./gccmvs -DUSE_MEMMGR -Os -S -ansi -pedantic-errors -DHAVE_CONFIG_H -DIN_GCC -DPUREISO $* -I ../../pdos/pdpclib -I . -I config/i370 -I ../include $file.c

done

